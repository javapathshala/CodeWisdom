<?xml version="1.0"?>


<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field">
	<class name="SetMapping.UserAccountsPersist"
		table="User_Accounts_Persist">
		<id name="id" column="id"
			type="integer">
			<generator class="increment" />
		</id>
			<property name="sortCode" column="SORT_CODE"/>
			<property name="accountNumber" column="PRODUCT_NUMBER" />
	</class>

</hibernate-mapping>
