<?xml version="1.0" encoding="UTF-8"?>
<project basedir="." default="compile" name="Ant">
	<property name="src" value="."/>
	<property name="build" value="build"/>
	<target name="init">
		<mkdir dir="${build}"/>
	</target>
	<target depends="init" name="compile">
		<javac destdir="${build}" srcdir="${src}"/>
</target>
</project>
